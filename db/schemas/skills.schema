# -*- mode: ruby -*-
# vi: set ft=ruby :
create_table "skills", force: :cascade do |t|
  t.text "title"
  t.text "experience"
  t.string "career_length"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.bigint "category_id", null: true
  t.bigint "product_id", null: true
end
